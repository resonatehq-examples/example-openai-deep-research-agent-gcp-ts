    "openai": "^6.8.1"
  },
  "devDependencies": {
    "@types/node": "^24.10.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "@google-cloud/functions-framework": "^4.0.0"
  }
}
